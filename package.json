    "karma-firefox-launcher": "~1.0.0",
    "karma-ie-launcher": "~1.0",
    "karma-mocha": "^1.1.1",
    "karma-requirejs": "^1.0.0",
    "karma-sauce-launcher": "^1.0.0",
    "karma-story-reporter": "~0.3.1",
    "load-grunt-tasks": "^3.5.0",
    "lodash": "^4.13.1",
    "mocha": "^2.5.3",
    "requirejs": "2.2.0",
    "semver": "^5.3.0",
    "tslint": "^3.13.0",
    "typescript": "^2.0.0",
