    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-coveralls": "^1.0.1",
    "grunt-karma": "~2.0.0",
    "grunt-mocha-istanbul": "^5.0.1",
    "grunt-run": "^0.6.0",
    "grunt-text-replace": "^0.4.0",
    "grunt-ts": "~5.5.1",
    "grunt-tslint": "~3.1.0",
    "grunt-typedoc": "^0.2.4",
